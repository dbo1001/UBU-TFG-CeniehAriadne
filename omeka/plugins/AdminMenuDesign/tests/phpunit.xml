<phpunit bootstrap="bootstrap.php"
         convertErrorsToExceptions="true"
         convertWarningsToExceptions="true">
    <testsuites>
        <testsuite name="Admin Menu Design Tests">
            <directory>suite</directory>
        </testsuite>
    </testsuites>
    <filter>
        <whitelist addUncoveredFilesFromWhitelist="true">
            <directory>../AdminMenuDesignPlugin.php</directory>
        </whitelist>
    </filter>
    <logging>
        <log type="coverage-html" target="/tmp/report/AdminMenuDesign" lowUpperBound="35"
            highLowerBound="70"/>
    </logging>
</phpunit>
