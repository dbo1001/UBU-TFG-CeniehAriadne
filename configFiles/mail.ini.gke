[ariadne-plus-tracking]

imap.host = ${IMAP_HOST} ; hostname or ip address of IMAP server [optional, default = 'localhost']
imap.username = ${IMAP_USERNAME} ; username
imap.password = ${IMAP_PASSWORD}; password for user 'username' [optional, default = '']
imap.port = ${IMAP_PORT}; port for IMAP server [optional, default = 110]
imap.ssl = ${IMAP_SSL} ; SSL / TLS
